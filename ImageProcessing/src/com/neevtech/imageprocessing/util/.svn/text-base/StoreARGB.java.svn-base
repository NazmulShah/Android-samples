package com.neevtech.imageprocessing.util;
/**
 * @author Shruti
 */
public class StoreARGB {
	private int pixel;
	private int A, R, G, B;
	private int x, y;

	public StoreARGB() {
		this.pixel = 0;
		this.A = 0;
		this.B = 0;
		this.G = 0;
		this.R = 0;
		this.x = 0;
		this.y = 0;
	}

	public int getPixel() {
		return pixel;
	}

	public void setPixel(int pixel) {
		this.pixel = pixel;
	}

	public int getA() {
		return A;
	}

	public void setA(int a) {
		A = a;
	}

	public int getR() {
		return R;
	}

	public void setR(int r) {
		R = r;
	}

	public int getG() {
		return G;
	}

	public void setG(int g) {
		G = g;
	}

	public int getB() {
		return B;
	}

	public void setB(int b) {
		B = b;
	}

	public int getX() {
		return x;
	}

	public void setX(int x) {
		this.x = x;
	}

	public int getY() {
		return y;
	}

	public void setY(int y) {
		this.y = y;
	}
}
